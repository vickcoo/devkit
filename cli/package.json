{
  "name": "blueprintkit",
  "version": "0.1.1",
  "description": "Scaffold production-ready projects in seconds with AI-powered templates. Next.js, React Native, Express, NestJS, and 15+ more frameworks.",
  "main": "dist/index.js",
  "bin": {
    "blueprintkit": "dist/index.js",
    "bpkit": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "copy-templates": "rm -rf templates && cp -r ../templates .",
    "prepublishOnly": "npm run copy-templates && npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "scaffold",
    "scaffolding",
    "template",
    "boilerplate",
    "generator",
    "blueprintkit",
    "nextjs",
    "react",
    "vue",
    "express",
    "nestjs",
    "fastapi",
    "react-native",
    "typescript",
    "starter",
    "project-generator",
    "code-generator",
    "productivity"
  ],
  "author": "Vick",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vickcoo/BlueprintKit.git"
  },
  "bugs": {
    "url": "https://github.com/vickcoo/BlueprintKit/issues"
  },
  "homepage": "https://blueprintkit.dev",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^8.2.5",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "fs-extra": "^11.2.0",
    "axios": "^1.6.5",
    "degit": "^2.8.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/validate-npm-package-name": "^4.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/degit": "^2.8.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
